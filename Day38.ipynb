   "source": [
    "def fib(n):\n",
    "\n",
    "   if n <= 1:\n",
    "\n",
    "       return n\n",
    "\n",
    "   else:\n",
    "\n",
    "       return fib(n-1) + fib(n-2)\n",
    "\n",
    "print(fib(7))"
   ]
