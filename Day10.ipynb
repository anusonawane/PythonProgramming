   ],
   "source": [
    "def fibonacci_easy(n):\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        print(a, end=\" \")\n",
    "        a, b = b, a + b\n",
    "\n",
    "# Test the function\n",
    "n = 10\n",
    "fibonacci_easy(n)"
   ]
