   "source": [
    "Write a Python program to find the common elements between two lists."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "c4d1f8ec-0b0e-48ad-801a-288e4544bc2b",
   "metadata": {},
   "outputs": [],
   "source": [
    "list_a = [1, 2, 3, 4, 5]\n",
    "list_b = [4, 5, 6, 7, 8]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "eb62ea75-83c3-45ac-a010-2e7ffdda154b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[4, 5]"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "\n",
    "common=[]\n",
    "for i in list_a:\n",
    "    if i in list_b:\n",
    "        common.append(i)\n",
    "\n",
    "common"
   ]
  },
